<?xml version="1.0" ?>

<container
        xmlns="http://symfony.com/schema/dic/services"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="cmf_seo.sitemap.controller.class">Symfony\Cmf\Bundle\SeoBundle\Sitemap\SitemapController</parameter>
        <parameter key="cmf_seo.sitemap.chain_provider.class">Symfony\Cmf\Bundle\SeoBundle\Sitemap\ChainProvider</parameter>
    </parameters>

    <services>
        <service id="cmf_seo.sitemap.controller" class="%cmf_seo.sitemap.controller.class%">
            <argument>cmf_seo.sitemap.chain_provider</argument>
        </service>
        <service id="cmf_seo.sitemap.chain_provider" class="%cmf_seo.sitemap.chain_provider.class%" />
    </services>
</container>
